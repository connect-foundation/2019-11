{
  "name": "todo-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:start": "cross-env NODE_ENV=development nodemon ./dist/server.js --name 'web'",
    "dev:build": "tsc -w",
    "dev": "concurrently npm:dev:*",
    "start": "rm -rf dist && tsc && cross-env NODE_ENV=development pm2-runtime dist/server.js --name 'web' ",
    "build": "tsc",
    "prod": "rm -rf dist && tsc && cross-env NODE_ENV=production pm2-runtime dist/server.js --name 'web' ",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "aws-sdk": "^2.535.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "concurrently": "^5.0.0",
    "connect-redis": "^4.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "ejs": "~2.6.1",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "handlebars": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.7.0",
    "nodemon": "^1.19.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.19",
    "typeorm-seeding": "^1.0.0-rc.3",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.6",
    "@types/http-errors": "^1.6.2",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.7.9",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^3.4.5",
    "cross-env": "^6.0.3"
  }
}
